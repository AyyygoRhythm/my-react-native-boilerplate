{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "clean": "rm -rf build  && mkdir build",
    "build-babel": "babel src --out-dir build --source-maps inline",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node -r node_modules/dotenv/config ./build/index.js",
    "babel-node": "babel-node -r node_modules/dotenv/config --inspect",
    "dev": "nodemon --exec npm run babel-node -- ./src/index.js",
    "sudo-dev": "sudo npm run dev",
    "knex": "npm run build && knex --knexfile build/knexfile.js",
    "start-db": "docker-compose up mysql",
    "migrate-db": "npm run knex -- migrate:latest",
    "rollback-db": "npm run knex -- migrate:rollback",
    "update-db": "npm run build && knex --knexfile build/knexfile.js migrate:rollback && knex --knexfile build/knexfile.js migrate:latest",
    "lint": "eslint \"./**/*.{js,jsx}\"",
    "lint:watch": "esw -w -ext .js --ext .jsx --clear .",
    "lint:fix": "npm run lint -- --fix",
    "lint:fix:dryrun": "npm run lint -- --fix-dry-run",
    "lint:diff": "git diff --name-only --relative | grep '\\.js$' | xargs eslint"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^10.11.4",
    "cookie-parser": "^1.4.3",
    "debug": "^4.0.1",
    "dotenv": "^6.1.0",
    "express": "^4.16.3",
    "express-list-endpoints": "^3.0.1",
    "express-winston": "github:bithavoc/express-winston",
    "http-errors": "^1.7.1",
    "jsonwebtoken": "^8.3.0",
    "knex": "^0.15.2",
    "machine-as-action": "^10.2.0",
    "module-alias": "^2.1.0",
    "morgan": "^1.9.1",
    "morgan-debug": "^2.0.0",
    "mysql": "^2.16.0",
    "objection": "^1.3.0",
    "objection-password": "^2.0.0",
    "objection-unique": "^1.0.1",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-facebook-token": "^3.3.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-google-token": "^0.1.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.12",
    "request-param": "^1.0.1",
    "typeorm": "^0.2.7",
    "winston": "^3.1.0",
    "winston-console-formatter": "^1.0.0-beta.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-watch": "^2.0.7"
  }
}
